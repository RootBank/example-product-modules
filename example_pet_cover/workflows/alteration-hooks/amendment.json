[
  {
    "type": "list",
    "label": "Pets",
    "maxNumber": 5,
    "components": [
      {
        "type": "section-header",
        "key": "header",
        "label": "Pet"
      },
      {
        "type": "text",
        "key": "name",
        "label": "Pet's name"
      },
      {
        "key": "cover_amount",
        "type": "currency-slider",
        "label": "Select the annual cover amount for your pet's vet fees",
        "props": {
          "prefix": "£",
          "increment": 100000
        },
        "validators": [
          {
            "validation": {
              "type": "required"
            }
          },
          {
            "validation": {
              "min": 100000,
              "type": "greaterThanCurrency"
            }
          },
          {
            "validation": {
              "max": 1000000,
              "type": "lessThanCurrency"
            }
          }
        ]
      },
      {
        "key": "excess_amount",
        "type": "currency-slider",
        "label": "Select the excess fee you will be responsible for when you claim?",
        "props": {
          "prefix": "£",
          "increment": 5000
        },
        "validators": [
          {
            "validation": {
              "type": "required"
            }
          },
          {
            "validation": {
              "min": 5000,
              "type": "greaterThanCurrency"
            }
          },
          {
            "validation": {
              "max": 25000,
              "type": "lessThanCurrency"
            }
          }
        ]
      },
      {
        "type": "horizontal-line",
        "key": "line"
      }
    ],
    "outputPathList": "pets"
  }
]
